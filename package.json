{
	"name": "latex-snippets-jeff",
	"displayName": "LaTeX Snippets",
	"description": "Useful Snippets for LaTeX",
	"version": "1.1.0",
	"publisher": "JeffersonQin",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "latex",
				"path": "./snippets/latex.json"
			}
        ],
        "commands": [
            {
                "title": "LaTeX Plotting Tool",
                "command": "extension.openWebview"
            }
        ]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JeffersonQin/LaTeX-Snippets-Jeff"
	},
	"homepage": "https://github.com/JeffersonQin/VSCode-LaTeX-Snippets/blob/master/README.md",
	"dependencies": {
		"js-polynomial-regression": "^0.9.1"
	}
}
