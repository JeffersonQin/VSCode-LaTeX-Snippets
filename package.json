{
	"name": "latex-snippets-jeff",
	"displayName": "LaTeX Snippets",
	"description": "Useful Snippets for LaTeX",
	"version": "1.2.3",
	"publisher": "JeffersonQin",
	"engines": {
		"vscode": "^1.45.1"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:tex",
		"onLanguage:latex",
		"onLanguage:doctex",
		"onLanguage:bibtex",
		"onCommand:latex-snippets.plot"
	],
	"main": "./src/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "latex",
				"path": "./snippets/latex.json"
			}
		],
		"commands": [
			{
				"title": "LaTeX Plotting Tool",
				"command": "latex-snippets.plot"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.2",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.47",
		"@types/vscode": "^1.45.1",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"typescript": "^3.9.5",
		"vscode-test": "^1.4.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JeffersonQin/LaTeX-Snippets-Jeff"
	},
	"homepage": "https://github.com/JeffersonQin/VSCode-LaTeX-Snippets/blob/master/README.md",
	"dependencies": {
		"js-polynomial-regression": "^0.9.1"
	},
	"icon": "lib/images/icon-128.png"
}
